# URL Rewriting for custom router
RewriteEngine On

# Adjust base to your subfolder
RewriteBase /livreor/public/

# Serve existing files/directories directly
RewriteCond %{REQUEST_FILENAME} -f [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^ - [L]

# Route everything else to index.php and pass the path as ?url=...
RewriteRule ^(.*)$ index.php?url=$1 [QSA,L]
